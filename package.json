{
  "name": "gps-to-gpx",
  "version": "1.0.0",
  "description": "A simple tool for converting an array of GPS points to GPX.",
  "keywords": [
    "gps",
    "gpx",
    "nike+",
    "nikeplus",
    "strava"
  ],
  "homepage": "https://github.com/impatrickhooper/gps-to-gpx#readme",
  "bugs": {
    "url": "https://github.com/impatrickhooper/gps-to-gpx/issues"
  },
  "license": "MIT",
  "author": "Patrick Hooper <patrick.d.hooper@gmail.com> (http://patrick.hoopr.io)",
  "files": [
    "CHANGELOG.md",
    "dist",
    "es",
    "lib",
    "LICENSE",
    "README.md",
    "src"
  ],
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/impatrickhooper/gps-to-gpx"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:commonjs": "npm run clean:commonjs && cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "npm run clean:es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "npm run clean:umd && cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/gps-to-gpx.js && cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/gps-to-gpx.min.js",
    "clean": "npm-run-all --parallel clean:**",
    "clean:commonjs": "rimraf lib",
    "clean:coverage": "rimraf .nyc_output coverage",
    "clean:es": "rimraf es",
    "clean:umd": "rimraf dist",
    "coverage": "npm run clean:coverage && nyc --reporter=lcov --reporter=text npm test",
    "lint": "eslint examples src test",
    "prepublish": "not-in-install && (npm run validate && npm run build) || in-install",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "validate": "npm-run-all --parallel lint coverage",
    "watch:src": "cross-env BABEL_ENV=commonjs babel src --watch --out-dir lib",
    "watch:test": "npm test -- --watch"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "loose-envify": "1.2.0",
    "xmlbuilder": "8.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.10.3",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "cross-env": "1.0.8",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "in-publish": "2.0.0",
    "mocha": "2.5.3",
    "npm-run-all": "2.3.0",
    "nyc": "7.0.0",
    "rimraf": "2.5.3",
    "webpack": "2.1.0-beta.15"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
